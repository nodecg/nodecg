{
	"source": {
		"include": [
			"./lib/api.js",
			"./README.md"
		]
	},
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": [
			"jsdoc"
		]
	},
	"plugins": [
		"plugins/markdown"
	],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": true,
		"github": {
			"url": "https://github.com/nodecg/nodecg"
		}
	},
	"opts": {
		"encoding": "utf8",
		"private": true,
		"recurse": true,
		"template": "./node_modules/minami",
		"tutorials": "./tutorials"
	},
	"markdown": {
		"idInHeadings": true
	}
}
