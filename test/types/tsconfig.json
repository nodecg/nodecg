{
	"compilerOptions": {
		"target": "esnext",
		"module": "commonjs",
		"lib": ["esnext", "dom"],
		"noEmit": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true
	}
}
