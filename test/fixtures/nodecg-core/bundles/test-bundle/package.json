{
  "name": "test-bundle",
  "version": "0.0.1",
  "dependencies": {
    "commander": "^2.6.0",
    "express": "^4.15.0"
  },
  "homepage": "http://github.com/nodecg",
  "authors": [
    "Alex Van Camp <email@alexvan.camp>",
    "Matt McNamara"
  ],
  "description": "A test bundle",
  "license": "MIT",
  "nodecg": {
    "compatibleRange": "^1.0.0",
    "assetCategories": [
      {
        "name": "assets",
        "title": "Assets",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png"
        ]
      }
    ],
    "soundCues": [
      {
        "name": "name-only"
      },
      {
        "name": "default-volume",
        "defaultVolume": 80
      },
      {
        "name": "non-assignable",
        "assignable": false
      },
      {
        "name": "default-file",
        "defaultFile": "sound/default-file.ogg"
      }
    ],
    "dashboardPanels": [
      {
        "name": "test",
        "title": "Test Panel",
        "width": 2,
        "file": "panel.html"
      },
      {
        "name": "test-dialog",
        "title": "Test Dialog",
        "width": 2,
        "file": "dialogs/dialog.html",
        "dialog": true,
        "dialogButtons": [
          {
            "name": "close",
            "type": "dismiss"
          },
          {
            "name": "accept",
            "type": "confirm"
          }
        ]
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1280,
        "height": 720
      },
      {
        "file": "single_instance.html",
        "width": 1280,
        "height": 720,
        "singleInstance": true
      }
    ],
    "mount": [
      {
        "directory": "custom-mount-folder",
        "endpoint": "custom-mount"
      }
    ]
  }
}
