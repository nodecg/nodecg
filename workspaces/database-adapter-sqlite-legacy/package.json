{
  "name": "@nodecg/database-adapter-sqlite-legacy",
  "version": "2.6.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodecg/nodecg.git",
    "directory": "workspaces/database-adapter-sqlite-legacy"
  },
  "exports": {
    ".": {
      "source": "./src/database-adapter.ts",
      "default": "./dist/database-adapter.js"
    }
  },
  "main": "dist/database-adapter.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "del-cli dist && tsc -b",
    "dev": "del-cli dist && tsc -b --watch --preserveWatchOutput",
    "generate-migrations": "typeorm-ts-node-commonjs migration:generate ./src/migration/rename-me -d ./src/datasource.ts"
  },
  "dependencies": {
    "@nodecg/database-adapter-types": "workspace:^",
    "@nodecg/internal-util": "workspace:^",
    "@types/better-sqlite3": "^7.6.13",
    "better-sqlite3": "^12.4.1",
    "typeorm": "0.3.27"
  },
  "devDependencies": {
    "del-cli": "^6.0.0",
    "typescript": "~5.7.3"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org"
  }
}
